
begin
   !*** Inspection Samples ***;
   
   class A; begin integer i; i:=aa; end;
   A class B; begin integer m; m:=i; end;
   ref(A) procedure F(g); integer g;
   begin F:=if(g>15) then x else y; end;
      
   ref(A) x; ref(B) y;
%   ref(A) z;
   integer aa;
   boolean bb;
   
   inspect F(54)
%   inspect x
      do i:=y.i+1 otherwise aa:=4;
   
   inspect F(13)  
%  inspect if bb then x else y
%   z:- if bb then x else y;
%   inspect z
   when B do
   begin 
      integer k;
      k:=i+4;
   end
   when A do
   begin 
      integer k;
      k:=i+16;
   end
   otherwise
   begin integer w;
      w:=w+1;
   end;
end;  