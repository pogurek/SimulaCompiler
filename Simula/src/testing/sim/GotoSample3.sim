begin
   integer lsc,count;
   procedure p(b,k); boolean b; integer k;
   begin
      M:N:count:=count+1;
      if count>6 then goto EXIT;
%      M: outtext("At Label M"); outimage;
%      N: outtext("At Label N"); outimage;
      if k>0 then p(not b,k-1) else
      begin
         if b then goto L;
         outtext("");
         if k>4 then
            goto if b then N else M;
      end;   
   end;
   L: 
   outtext("Outer LOOP"); outimage;
   p(false,3);
   count:=count+1;
   if count>6 then goto EXIT;
   goto L;
EXIT:end;   